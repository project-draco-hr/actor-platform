{
  int j=pt;
  for (int i=0; i < length; i++) {
    m.setByte(j--,in[offset + i]);
    if (j < 0) {
      StreebogMath.streebog_g(h,m,n);
      n+=0x200;
      int c=0;
      for (j=63; j >= 0; j--) {
        c+=(e.getByte(j) & 0xFF) + (m.getByte(j) & 0xFF);
        e.setByte(j,(byte)(c & 0xFF));
        c>>=8;
      }
      j=63;
    }
  }
  pt=j;
}
