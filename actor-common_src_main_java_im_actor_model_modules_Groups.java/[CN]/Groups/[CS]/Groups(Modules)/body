{
  super(modules);
  collection=new MVVMCollection<Group,GroupVM>(modules.getConfiguration().getStorage().createGroupsEngine()){
    @Override protected GroupVM createNew(    Group raw){
      return new GroupVM(raw);
    }
    @Override protected byte[] serialize(    Group raw){
      return raw.toByteArray();
    }
    @Override protected Group deserialize(    byte[] raw){
      try {
        return Group.fromBytes(raw);
      }
 catch (      IOException e) {
        e.printStackTrace();
        return null;
      }
    }
  }
;
  groups=collection.getEngine();
}
