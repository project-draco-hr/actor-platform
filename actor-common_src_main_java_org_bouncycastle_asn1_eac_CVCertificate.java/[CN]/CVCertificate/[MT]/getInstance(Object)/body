{
  if (obj instanceof CVCertificate) {
    return (CVCertificate)obj;
  }
 else   if (obj != null) {
    try {
      return new CVCertificate(DERApplicationSpecific.getInstance(obj));
    }
 catch (    IOException e) {
      throw new ASN1ParsingException("unable to parse data: " + e.getMessage(),e);
    }
  }
  return null;
}
