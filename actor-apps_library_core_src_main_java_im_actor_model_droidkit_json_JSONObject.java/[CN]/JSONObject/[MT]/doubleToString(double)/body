{
  if (Double.isInfinite(d) || Double.isNaN(d)) {
    return "null";
  }
  String string=Double.toString(d);
  if (string.contains(".") && !string.contains("e") && !string.contains("E")) {
    while (string.endsWith("0")) {
      string=string.substring(0,string.length() - 1);
    }
    if (string.endsWith(".")) {
      string=string.substring(0,string.length() - 1);
    }
  }
  return string;
}
