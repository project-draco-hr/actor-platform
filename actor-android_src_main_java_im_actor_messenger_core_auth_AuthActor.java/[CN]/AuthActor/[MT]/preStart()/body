{
  super.preStart();
  String android_id=Settings.Secure.getString(AppContext.getContext().getContentResolver(),Settings.Secure.ANDROID_ID);
  deviceId="android:" + AppContext.getContext().getPackageName() + ":"+ android_id;
  Log.d("AuthActor","DeviceID: " + deviceId);
  deviceIdHash=Crypto.SHA256(deviceId.getBytes());
  deviceName=Build.MANUFACTURER + " " + Build.MODEL;
  try {
    Cursor c=AppContext.getContext().getContentResolver().query(ContactsContract.Profile.CONTENT_URI,new String[]{"display_name"},null,null,null);
    if (c != null && c.moveToFirst()) {
      deviceName+=" (" + c.getString(c.getColumnIndex("display_name")) + ")";
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
