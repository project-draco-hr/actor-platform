{
  if (archivedLoaded) {
    return;
  }
  if (isLoading) {
    return;
  }
  isLoading=true;
  Log.d(TAG,"Loading archived");
  request(new RequestLoadArchived(nextOffset,LIMIT),new RpcCallback<ResponseLoadArchived>(){
    @Override public void onResult(    ResponseLoadArchived response){
      updates().onUpdateReceived(new ArchivedDialogLoaded(response));
    }
    @Override public void onError(    RpcException e){
      e.printStackTrace();
    }
  }
);
}
