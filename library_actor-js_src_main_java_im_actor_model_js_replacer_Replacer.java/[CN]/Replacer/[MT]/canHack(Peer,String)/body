{
  if (text.contains("www.dropbox.com")) {
    if (!text.trim().contains(" ")) {
      hackDropBox(peer,text.trim());
      return true;
    }
  }
  if (text.contains("drive.google.com")) {
    if (!text.trim().contains(" ")) {
      hackGoogleDrive(peer,text.trim());
      return true;
    }
  }
  return false;
}
