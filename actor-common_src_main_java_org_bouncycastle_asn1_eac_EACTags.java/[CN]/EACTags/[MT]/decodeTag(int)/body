{
  int retValue=0;
  boolean multiBytes=false;
  for (int i=24; i >= 0; i-=8) {
    int currentByte=tag >> i & 0xFF;
    if (currentByte == 0) {
      continue;
    }
    if (multiBytes) {
      retValue<<=7;
      retValue|=currentByte & 0x7F;
    }
 else     if ((currentByte & 0x1F) == 0x1F) {
      multiBytes=true;
    }
 else {
      return currentByte & 0x1F;
    }
  }
  return retValue;
}
