{
  new Thread(){
    @Override public void run(){
      try {
        Thread.sleep(PRELOAD_DELAY);
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
      ArrayList<PhoneBookContact> contacts=loadPhoneBook(AppContext.getContext(),CountryUtil.getDeviceCountry(AppContext.getContext()));
      callback.onLoaded(contacts);
    }
  }
.start();
}
