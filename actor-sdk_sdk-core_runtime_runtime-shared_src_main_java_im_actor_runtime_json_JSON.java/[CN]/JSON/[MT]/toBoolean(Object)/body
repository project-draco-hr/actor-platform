{
  if (value instanceof Boolean) {
    return (Boolean)value;
  }
 else   if (value instanceof String) {
    String stringValue=(String)value;
    if ("true".equalsIgnoreCase(stringValue)) {
      return true;
    }
 else     if ("false".equalsIgnoreCase(stringValue)) {
      return false;
    }
  }
  return null;
}
