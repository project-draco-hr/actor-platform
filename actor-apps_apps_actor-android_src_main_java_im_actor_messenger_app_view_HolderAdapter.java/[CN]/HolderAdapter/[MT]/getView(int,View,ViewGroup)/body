{
  V obj=getItem(position);
  ViewHolder<V> holder;
  View view;
  if (convertView == null || convertView.getTag() == null) {
    holder=createHolder(obj);
    view=holder.init(obj,parent,context);
    view.setTag(holder);
    holders.add(holder);
  }
 else {
    holder=(ViewHolder<V>)convertView.getTag();
    view=convertView;
  }
  holder.bind(obj,position,context);
  return view;
}
