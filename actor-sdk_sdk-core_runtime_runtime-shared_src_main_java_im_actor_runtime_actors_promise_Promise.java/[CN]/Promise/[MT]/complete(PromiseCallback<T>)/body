{
  if (isFinished) {
    im.actor.runtime.Runtime.dispatch(new Runnable(){
      @Override public void run(){
        if (exception != null) {
          callback.onError(exception);
        }
 else {
          callback.onResult(result);
        }
      }
    }
);
  }
 else {
    callbacks.add(callback);
  }
  return this;
}
