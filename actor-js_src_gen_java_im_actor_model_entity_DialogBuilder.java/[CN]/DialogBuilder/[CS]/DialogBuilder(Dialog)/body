{
  peer=dialog.getPeer();
  sortKey=dialog.getSortDate();
  dialogTitle=dialog.getDialogTitle();
  dialogAvatar=dialog.getDialogAvatar();
  unreadCount=dialog.getUnreadCount();
  rid=dialog.getRid();
