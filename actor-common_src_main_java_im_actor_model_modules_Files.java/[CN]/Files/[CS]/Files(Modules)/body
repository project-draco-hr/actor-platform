{
  super(modules);
  downloadedEngine=new BaseKeyValueEngine<Downloaded>(modules.getConfiguration().getStorage().createKeyValue(STORAGE_DOWNLOADS)){
    @Override protected byte[] serialize(    Downloaded value){
      return value.toByteArray();
    }
    @Override protected Downloaded deserialize(    byte[] data){
      try {
        return Downloaded.fromBytes(data);
      }
 catch (      IOException e) {
        e.printStackTrace();
        return null;
      }
    }
  }
;
}
