{
  boolean changed=false;
  char[] chars=string.toCharArray();
  for (int i=0; i != chars.length; i++) {
    char ch=chars[i];
    if ('A' <= ch && 'Z' >= ch) {
      changed=true;
      chars[i]=(char)(ch - 'A' + 'a');
    }
  }
  if (changed) {
    return new String(chars);
  }
  return string;
}
