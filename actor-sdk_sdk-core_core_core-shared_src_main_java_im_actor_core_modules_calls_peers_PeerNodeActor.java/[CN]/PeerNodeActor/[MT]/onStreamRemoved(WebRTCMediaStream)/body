{
  theirMediaStreams.remove(stream);
  if (isStarted) {
    for (    WebRTCMediaTrack track : stream.getAudioTracks()) {
      if (isAudioEnabled) {
        callback.onTrackRemoved(deviceId,track);
      }
    }
    for (    WebRTCMediaTrack track : stream.getVideoTracks()) {
      if (isVideoEnabled) {
        callback.onTrackRemoved(deviceId,track);
      }
    }
  }
}
