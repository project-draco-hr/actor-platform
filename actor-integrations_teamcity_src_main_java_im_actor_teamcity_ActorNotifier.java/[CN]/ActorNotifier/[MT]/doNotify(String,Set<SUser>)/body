{
  String jsonString=text.replace("\\","\\\\").replace("\"","\\\"");
  for (  SUser user : users) {
    String token=user.getPropertyValue(PROPERTY_TOKEN_ID);
    RequestBody body=RequestBody.create(MediaType.parse("application/json"),"{\"text\": \"" + jsonString + "\"}");
    Request request=new Request.Builder().url(token).post(body).build();
    try {
      client.newCall(request).execute();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
