{
  if (seq.size() < 1 || seq.size() > 2) {
    throw new IllegalArgumentException("Bad sequence size: " + seq.size());
  }
  objectId=ASN1ObjectIdentifier.getInstance(seq.getObjectAt(0));
  if (seq.size() == 2) {
    parametersDefined=true;
    parameters=seq.getObjectAt(1);
  }
 else {
    parameters=null;
  }
}
