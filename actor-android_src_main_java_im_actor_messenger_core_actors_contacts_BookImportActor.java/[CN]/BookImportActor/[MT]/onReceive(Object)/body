{
  if (!Core.isLoggedIn()) {
    return;
  }
  if (message instanceof StartSync) {
    AppContext.getContext().getContentResolver().registerContentObserver(ContactsContract.Contacts.CONTENT_URI,true,contentObserver);
    self().send(new PerformSync());
  }
 else   if (message instanceof PerformSync) {
    checkBook();
  }
}
