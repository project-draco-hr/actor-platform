{
  FrameLayout res=new FrameLayout(getContext());
  FrameLayout content=new FrameLayout(getContext());
  content.setId(R.id.content);
  res.addView(content,new FrameLayout.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.MATCH_PARENT));
  FrameLayout fab=new FrameLayout(getContext());
  fab.setId(R.id.fab);
  res.addView(fab,new FrameLayout.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.MATCH_PARENT));
  FrameLayout search=new FrameLayout(getContext());
  search.setId(R.id.search);
  res.addView(search,new FrameLayout.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.MATCH_PARENT));
  FrameLayout placeholder=new FrameLayout(getContext());
  placeholder.setId(R.id.placeholder);
  res.addView(placeholder,new FrameLayout.LayoutParams(ViewGroup.LayoutParams.MATCH_PARENT,ViewGroup.LayoutParams.MATCH_PARENT));
  return res;
}
