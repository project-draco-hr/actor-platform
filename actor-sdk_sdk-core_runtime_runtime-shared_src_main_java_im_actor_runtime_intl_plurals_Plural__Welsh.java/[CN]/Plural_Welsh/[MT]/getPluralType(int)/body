{
  if (value == 0) {
    return PluralType.ZERO;
  }
 else   if (value == 1) {
    return PluralType.ONE;
  }
 else   if (value == 2) {
    return PluralType.TWO;
  }
 else   if (value == 3) {
    return PluralType.FEW;
  }
 else   if (value == 6) {
    return PluralType.MANY;
  }
 else {
    return PluralType.OTHER;
  }
}
