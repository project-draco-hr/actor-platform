{
  Peer peer=convert(_peer);
  if (!isValidPeer(peer)) {
    return;
  }
  AbsContent content;
  try {
    content=AbsContent.fromMessage(message);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return;
  }
  conversationActor(peer).send(new ConversationActor.MessageContentUpdated(rid,content));
}
