{
  FileOutputStream outputStream=null;
  try {
    outputStream=new FileOutputStream(fileName);
    src.compress(format,quality,outputStream);
    outputStream.close();
  }
  finally {
    if (outputStream != null) {
      try {
        outputStream.close();
      }
 catch (      IOException e1) {
        e1.printStackTrace();
      }
    }
  }
}
