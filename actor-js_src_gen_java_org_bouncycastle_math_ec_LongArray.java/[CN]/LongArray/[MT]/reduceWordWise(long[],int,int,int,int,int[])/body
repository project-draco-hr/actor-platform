{
  int toPos=toBit >>> 6;
  while (--len > toPos) {
    long word=buf[off + len];
    if (word != 0) {
      buf[off + len]=0;
      reduceWord(buf,off,(len << 6),word,m,ks);
    }
  }
{
    int partial=toBit & 0x3F;
    long word=buf[off + toPos] >>> partial;
    if (word != 0) {
      buf[off + toPos]^=word << partial;
      reduceWord(buf,off,toBit,word,m,ks);
    }
  }
}
