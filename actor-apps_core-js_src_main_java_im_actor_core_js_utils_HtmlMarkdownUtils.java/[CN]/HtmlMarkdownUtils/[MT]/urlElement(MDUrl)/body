{
  String href=UriUtils.sanitizeUri(url.getUrl());
  if (href != "#" && !href.contains("://")) {
    href="http://" + href;
  }
  return "<a " + "target=\"_blank\" " + "onClick=\"window.messenger.handleLinkClick(event)\" "+ "href=\"" + href + "\">"+ SafeHtmlUtils.htmlEscape(url.getUrlTitle())+ "</a>";
}
