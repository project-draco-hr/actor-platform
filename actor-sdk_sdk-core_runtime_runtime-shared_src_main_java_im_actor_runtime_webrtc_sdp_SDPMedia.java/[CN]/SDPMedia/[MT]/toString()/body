{
  String res="m=" + type + " "+ codecs.size();
  String proto="";
  for (  String p : protocols) {
    if (proto.length() > 0) {
      proto+="/";
    }
    proto+=p;
  }
  res+=" " + proto;
  for (  Integer codec : codecs) {
    res+=" " + codec;
  }
  for (  String a : args) {
    res+=" " + a;
  }
  res+="\r\n";
  for (  SDPRawRecord r : records) {
    res+=r.toString() + "\r\n";
  }
  return res;
}
