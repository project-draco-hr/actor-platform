{
  ASN1EncodableVector outer=new ASN1EncodableVector();
  for (int i=0; i != certReqId.length; i++) {
    ASN1EncodableVector v=new ASN1EncodableVector();
    v.add(certReqId[i]);
    v.add(checkAfter[i]);
    if (reason[i] != null) {
      v.add(reason[i]);
    }
    outer.add(new DERSequence(v));
  }
  return new DERSequence(outer);
}
