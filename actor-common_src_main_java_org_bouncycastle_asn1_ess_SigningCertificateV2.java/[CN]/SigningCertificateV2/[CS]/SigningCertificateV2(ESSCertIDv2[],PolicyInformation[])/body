{
  ASN1EncodableVector v=new ASN1EncodableVector();
  for (int i=0; i < certs.length; i++) {
    v.add(certs[i]);
  }
  this.certs=new DERSequence(v);
  if (policies != null) {
    v=new ASN1EncodableVector();
    for (int i=0; i < policies.length; i++) {
      v.add(policies[i]);
    }
    this.policies=new DERSequence(v);
  }
}
