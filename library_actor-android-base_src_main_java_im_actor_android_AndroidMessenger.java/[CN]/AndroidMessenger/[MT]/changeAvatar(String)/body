{
  try {
    Bitmap bmp=ImageHelper.loadOptimizedHQ(descriptor);
    if (bmp == null) {
      return;
    }
    String resultFileName=getExternalTempFile("image","jpg");
    if (resultFileName == null) {
      return;
    }
    ImageHelper.save(bmp,resultFileName);
    super.changeAvatar(resultFileName);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
