{
  if (update instanceof UpdateIncomingCall) {
    UpdateIncomingCall updateIncomingCall=(UpdateIncomingCall)update;
    context.getCallsModule().getCallManager().send(new CallManagerActor.OnIncomingCall(updateIncomingCall.getCallId(),updateIncomingCall.getUid()));
    return true;
  }
 else   if (update instanceof UpdateCallSignal) {
    UpdateCallSignal updateCallSignal=(UpdateCallSignal)update;
    context.getCallsModule().getCallManager().send(new CallManagerActor.OnSignaling(updateCallSignal.getCallId(),updateCallSignal.getContent()));
    return true;
  }
 else   if (update instanceof UpdateCallEnded) {
    UpdateCallEnded callEnded=(UpdateCallEnded)update;
    context.getCallsModule().getCallManager().send(new CallManagerActor.OnCallEnded(callEnded.getCallId()));
    return true;
  }
 else   if (update instanceof UpdateCallInProgress) {
    return true;
  }
  return false;
}
