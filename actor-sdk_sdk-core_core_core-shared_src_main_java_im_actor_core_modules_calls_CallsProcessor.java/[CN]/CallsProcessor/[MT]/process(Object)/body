{
  if (update instanceof UpdateIncomingCall) {
    UpdateIncomingCall updateIncomingCall=(UpdateIncomingCall)update;
    if (context.getConfiguration().isVoiceCallsEnabled()) {
      context.getCallsModule().getCallManager().send(new CallManagerActor.OnIncomingCall(updateIncomingCall.getCallId()));
    }
    return true;
  }
 else   if (update instanceof UpdateCallHandled) {
    UpdateCallHandled updateCallHandled=(UpdateCallHandled)update;
    if (context.getConfiguration().isVoiceCallsEnabled()) {
      context.getCallsModule().getCallManager().send(new CallManagerActor.OnIncomingCallHandled(updateCallHandled.getCallId()));
    }
    return true;
  }
  return false;
}
