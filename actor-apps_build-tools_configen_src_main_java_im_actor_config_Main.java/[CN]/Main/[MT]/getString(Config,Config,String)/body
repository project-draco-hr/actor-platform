{
  if (altConfig != null) {
    if (altConfig.hasPath(path)) {
      if (!altConfig.getIsNull(path)) {
        return altConfig.getString(path);
      }
 else {
        return null;
      }
    }
  }
  if (config.hasPath(path)) {
    if (!config.getIsNull(path)) {
      return config.getString(path);
    }
  }
  return null;
}
