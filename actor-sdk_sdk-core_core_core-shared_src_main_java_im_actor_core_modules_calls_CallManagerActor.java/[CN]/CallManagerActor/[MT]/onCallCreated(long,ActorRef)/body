{
  if (currentCall != null) {
    ActorRef dest=runningCalls.remove(currentCall);
    if (dest != null) {
      dest.send(PoisonPill.INSTANCE);
    }
    if (isBeeping) {
      isBeeping=false;
      provider.stopOutgoingBeep();
    }
    currentCall=null;
  }
  runningCalls.put(callId,ref);
  currentCall=callId;
  provider.onCallStart(callId);
  isBeeping=true;
  provider.startOutgoingBeep();
}
