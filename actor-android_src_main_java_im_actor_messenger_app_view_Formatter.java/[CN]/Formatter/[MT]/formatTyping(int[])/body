{
  if (uids.length == 1) {
    UserModel u=users().get(uids[0]);
    if (u != null) {
      return AppContext.getContext().getString(R.string.typing_group).replace("{0}",u.getName());
    }
 else {
      return AppContext.getContext().getString(R.string.typing_private);
    }
  }
 else {
    return AppContext.getContext().getString(R.string.typing_group_many).replace("{0}","" + uids.length);
  }
}
