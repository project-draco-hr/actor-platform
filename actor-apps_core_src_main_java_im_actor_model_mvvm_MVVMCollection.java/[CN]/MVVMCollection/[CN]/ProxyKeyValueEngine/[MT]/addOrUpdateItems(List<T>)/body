{
  for (  T t : values) {
    cache.put(t.getEngineId(),t);
  }
  notifyChange(values);
  ArrayList<KeyValueRecord> records=new ArrayList<KeyValueRecord>();
  for (  T v : values) {
    records.add(new KeyValueRecord(v.getEngineId(),serialize(v)));
  }
  collectionStorage.addOrUpdateItems(records);
}
