{
  ArrayList<ContactRecord> res=new ArrayList<ContactRecord>();
  for (  im.actor.model.api.ContactRecord r : user.getContactInfo()) {
    if (r.getType() == ContactType.PHONE) {
      res.add(new ContactRecord(0,0,ContactRecord.TYPE_PHONE,r.getLongValue() + "",r.getSubtitle() != null ? r.getSubtitle() : "Mobile"));
    }
 else     if (r.getType() == ContactType.EMAIL) {
      res.add(new ContactRecord(0,0,ContactRecord.TYPE_EMAIL,r.getStringValue(),r.getSubtitle() != null ? r.getSubtitle() : "Email"));
    }
  }
  return new User(user.getId(),user.getAccessHash(),user.getName(),user.getLocalName(),convert(user.getAvatar()),convert(user.getSex()),res);
}
