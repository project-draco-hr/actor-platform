{
  Log.d(TAG,"-> request#" + randomId + ": "+ message);
  RequestHolder holder=new RequestHolder(Environment.getCurrentTime(),randomId,new RpcRequest(message.getHeaderKey(),message.toByteArray()),callback);
  requests.put(holder.publicId,holder);
  if (proto != null) {
    long mid=proto.sendRpcMessage(holder.message);
    holder.protoId=mid;
    idMap.put(mid,randomId);
  }
}
