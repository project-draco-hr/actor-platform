{
  boolean isUpdatedIndex=false;
  for (  Index i : indexes) {
    if (i.getId() == id) {
      indexes.remove(i);
      isUpdatedIndex=true;
      break;
    }
  }
  indexes.add(new Index(id,sortKey));
  storage.setItem(prefix + "_i_" + id,Base64Utils.toBase64(data));
  if (isUpdatedIndex) {
    updateIndex();
  }
}
