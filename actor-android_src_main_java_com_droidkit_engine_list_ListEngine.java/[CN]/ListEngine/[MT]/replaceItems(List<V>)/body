{
synchronized (LOCK) {
    cache.clear();
    for (    V val : values) {
      cache.onObjectUpdated(dataAdapter.getId(val),val);
    }
    cache.startLock();
    dbActor.clear();
    dbActor.updateOrAdd(values);
    dbRawActor.send(new Runnable(){
      @Override public void run(){
        cache.stopLock();
      }
    }
);
    for (    ListEngineCallback<V> callback : listeners) {
      callback.onItemsReplaced(values);
    }
  }
}
