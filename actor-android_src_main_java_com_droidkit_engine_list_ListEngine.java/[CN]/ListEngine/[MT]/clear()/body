{
synchronized (LOCK) {
    cache.clear();
    cache.startLock();
    dbActor.clear();
    dbRawActor.send(new Runnable(){
      @Override public void run(){
        cache.stopLock();
      }
    }
);
    for (    ListEngineCallback<V> callback : listeners) {
      callback.onListClear();
    }
  }
}
