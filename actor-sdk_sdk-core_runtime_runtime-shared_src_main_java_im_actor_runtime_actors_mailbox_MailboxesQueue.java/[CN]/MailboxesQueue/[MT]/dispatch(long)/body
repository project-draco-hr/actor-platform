{
  EnvelopeRoot.FetchResult res=envelopeRoot.fetchCollection(time);
  if (res == null) {
    return delay(FOREVER);
  }
  if (res.getEnvelope() != null) {
    DispatchResult result=result(res.getEnvelope());
    res.recycle();
    return result;
  }
 else {
    DispatchResult result=delay(res.getDelay());
    res.recycle();
    return result;
  }
}
