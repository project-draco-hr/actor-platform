{
  super(context);
  this.dialogDescKeyValue=Storage.createKeyValue(STORAGE_DIALOGS_DESC,DialogSpecVM.CREATOR,DialogSpec.CREATOR);
  this.cursorStorage=new SyncKeyValue(Storage.createKeyValue(STORAGE_CURSOR));
