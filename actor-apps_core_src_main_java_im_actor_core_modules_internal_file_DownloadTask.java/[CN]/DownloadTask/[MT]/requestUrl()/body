{
  if (LOG) {
    Log.d(TAG,"Loading url...");
  }
  request(new RequestGetFileUrl(new ApiFileLocation(fileReference.getFileId(),fileReference.getAccessHash())),new RpcCallback<ResponseGetFileUrl>(){
    @Override public void onResult(    ResponseGetFileUrl response){
      fileUrl=response.getUrl();
      if (LOG) {
        Log.d(TAG,"Loaded file url: " + fileUrl);
      }
      startDownload();
    }
    @Override public void onError(    RpcException e){
      if (LOG) {
        Log.d(TAG,"Unable to load file url");
      }
      reportError();
    }
  }
);
}
