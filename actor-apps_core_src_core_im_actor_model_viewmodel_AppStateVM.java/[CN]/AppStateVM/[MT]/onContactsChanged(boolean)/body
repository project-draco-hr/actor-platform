{
  if (isContactsEmpty.get() != isEmpty) {
    modules.getPreferences().putBool("app.contacts.empty",isEmpty);
    isContactsEmpty.change(isEmpty);
  }
  if (!isEmpty) {
    if (isAppEmpty.get()) {
      modules.getPreferences().putBool("app.empty",false);
      isAppEmpty.change(false);
    }
  }
}
