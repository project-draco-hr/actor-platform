{
  if (historyLoaded) {
    return;
  }
  if (isLoading) {
    return;
  }
  isLoading=true;
  request(new RequestLoadHistory(buidOutPeer(peer),historyMaxDate,null,LIMIT),new RpcCallback<ResponseLoadHistory>(){
    @Override public void onResult(    ResponseLoadHistory response){
      updates().onUpdateReceived(new MessagesHistoryLoaded(peer,response));
    }
    @Override public void onError(    RpcException e){
      e.printStackTrace();
    }
  }
);
}
