{
  if (update instanceof UpdateMessage || update instanceof UpdateMessageRead || update instanceof UpdateMessageReadByMe|| update instanceof UpdateMessageReceived|| update instanceof UpdateMessageDelete|| update instanceof UpdateMessageContentChanged|| update instanceof UpdateChatClear|| update instanceof UpdateChatDelete|| update instanceof UpdateChatGroupsChanged|| update instanceof UpdateReactionsUpdate|| update instanceof UpdateMessageSent) {
    return context().getMessagesModule().getRouter().onUpdate(update);
  }
  return null;
}
