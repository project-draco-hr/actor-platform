{
  if (a == b) {
    return true;
  }
  if (a == null || b == null) {
    return false;
  }
  if (a.length != b.length) {
    return false;
  }
  for (int i=0; i != a.length; i++) {
    Object objA=a[i], objB=b[i];
    if (objA == null) {
      if (objB != null) {
        return false;
      }
    }
 else     if (!objA.equals(objB)) {
      return false;
    }
  }
  return true;
}
