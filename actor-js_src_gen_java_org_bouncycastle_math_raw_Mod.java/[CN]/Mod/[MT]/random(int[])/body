{
  int len=p.length;
  Random rand=new Random();
  int[] s=Nat.create(len);
  int m=p[len - 1];
  m|=m >>> 1;
  m|=m >>> 2;
  m|=m >>> 4;
  m|=m >>> 8;
  m|=m >>> 16;
  do {
    for (int i=0; i != len; i++) {
      s[i]=rand.nextInt();
    }
    s[len - 1]&=m;
  }
 while (Nat.gte(len,s,p));
  return s;
}
