{
  BserValues values=new BserValues(BserParser.deserialize(new DataInput(src,0,src.length)));
  int key=values.getInt(1);
  byte[] content=values.getBytes(2);
switch (key) {
case 1:
    return Bser.parse(new TextMessage(),content);
case 2:
  return Bser.parse(new ServiceMessage(),content);
case 3:
return Bser.parse(new DocumentMessage(),content);
default :
return new MessageUnsupported(key,content);
}
}
