{
  try {
    if (message instanceof Update) {
      Update u=(Update)message;
      onUpdate(u);
    }
 else     if (message instanceof List) {
      for (      Update u : (List<Update>)message) {
        onUpdate(u);
      }
    }
 else {
      drop(message);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
