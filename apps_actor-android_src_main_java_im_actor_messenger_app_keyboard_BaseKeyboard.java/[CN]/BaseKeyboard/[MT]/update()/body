{
  if (emojiKeyboardView != null) {
    WindowManager.LayoutParams params=new WindowManager.LayoutParams(WindowManager.LayoutParams.MATCH_PARENT,(keyboardHeight),WindowManager.LayoutParams.TYPE_PHONE,WindowManager.LayoutParams.FLAG_NOT_FOCUSABLE,PixelFormat.TRANSLUCENT);
    params.gravity=Gravity.BOTTOM | Gravity.CENTER_HORIZONTAL;
    windowManager.updateViewLayout(emojiKeyboardView,params);
  }
 else {
  }
}
