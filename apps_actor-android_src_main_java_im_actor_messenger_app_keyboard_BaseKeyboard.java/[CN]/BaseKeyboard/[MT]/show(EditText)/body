{
  this.messageBody=messageBody;
  showing=true;
  dismissed=false;
  if (softwareKeyboardShowing) {
    showInternal();
  }
 else {
    messageBody.setFocusableInTouchMode(true);
    messageBody.requestFocus();
    inputMethodManager.showSoftInput(messageBody,InputMethodManager.SHOW_IMPLICIT);
  }
}
