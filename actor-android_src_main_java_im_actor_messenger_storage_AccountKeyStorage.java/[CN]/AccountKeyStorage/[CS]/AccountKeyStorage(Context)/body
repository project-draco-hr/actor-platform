{
  fileWriter=new SafeFileWriter(context,"key.bin");
  byte[] data=fileWriter.loadData();
  if (data != null && data.length > 0) {
    try {
      key=Bser.parse(AccountKey.class,data);
      if (key.getPrivateKey() != null) {
        keyPair=new KeyPair(KeyTools.decodeRsaPublicKey(key.getPublicKey()),KeyTools.decodeRsaPrivateKey(key.getPrivateKey()));
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
      key=new AccountKey();
    }
  }
  if (key == null) {
    key=new AccountKey();
  }
}
