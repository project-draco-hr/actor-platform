{
  ASN1EncodableVector v=new ASN1EncodableVector();
  if (crlUrl != null) {
    v.add(new DERTaggedObject(true,0,crlUrl));
  }
  if (crlNum != null) {
    v.add(new DERTaggedObject(true,1,crlNum));
  }
  if (crlTime != null) {
    v.add(new DERTaggedObject(true,2,crlTime));
  }
  return new DERSequence(v);
}
