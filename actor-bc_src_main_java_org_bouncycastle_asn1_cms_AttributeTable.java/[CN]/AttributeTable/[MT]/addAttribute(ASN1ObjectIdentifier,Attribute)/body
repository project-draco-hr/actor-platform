{
  Object value=attributes.get(oid);
  if (value == null) {
    attributes.put(oid,a);
  }
 else {
    Vector v;
    if (value instanceof Attribute) {
      v=new Vector();
      v.addElement(value);
      v.addElement(a);
    }
 else {
      v=(Vector)value;
      v.addElement(a);
    }
    attributes.put(oid,v);
  }
}
