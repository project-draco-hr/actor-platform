{
  messages=messages(peer);
  messagesStorage=new PendingMessagesStorage();
  byte[] data=preferences().getBytes("conv_pending_" + peer.getUid());
  if (data != null) {
    try {
      messagesStorage=PendingMessagesStorage.fromBytes(data);
