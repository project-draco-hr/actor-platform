{
  outer:   for (int i=0; i < sourceList.size(); i++) {
    long id=sourceList.get(i).getEngineId();
    for (    T itm : items) {
      if (itm.getEngineId() == id) {
        continue outer;
      }
    }
    result.appendRemove(i,1);
    sourceList.remove(i);
    i--;
  }
  for (  T itm : items) {
    addOrUpdate(itm,sourceList,result,false);
  }
}
