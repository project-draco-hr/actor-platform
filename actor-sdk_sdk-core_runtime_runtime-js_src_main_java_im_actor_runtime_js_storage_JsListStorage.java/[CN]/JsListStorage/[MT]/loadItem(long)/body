{
  Index indexValue=null;
  for (  Index i : index) {
    if (i.id == key) {
      indexValue=i;
      break;
    }
  }
  if (indexValue == null) {
    return null;
  }
  String item=storage.getItem(getId(key));
  if (item != null) {
    byte[] res=fromBase64(item);
    return new ListEngineRecord(key,indexValue.sortKey,null,res);
  }
  return null;
}
