{
  for (int i=0; i < index.size(); i++) {
    Index ind=index.get(i);
    if (ind.id == id) {
      index.remove(i);
      break;
    }
  }
  boolean found=false;
  for (int i=0; i < index.size(); i++) {
    Index ind=index.get(i);
    if (ind.sortKey < sortKey) {
      found=true;
      index.add(i,new Index(id,sortKey));
    }
  }
  if (!found) {
    index.add(new Index(id,sortKey));
  }
}
