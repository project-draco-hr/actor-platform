{
  long cc=0;
  if (x != 0) {
    long xx07=x & M;
    cc+=(z[0] & M) - xx07;
    z[0]=(int)cc;
    cc>>=32;
    if (cc != 0) {
      cc+=(z[1] & M);
      z[1]=(int)cc;
      cc>>=32;
      cc+=(z[2] & M);
      z[2]=(int)cc;
      cc>>=32;
    }
    cc+=(z[3] & M) + xx07;
    z[3]=(int)cc;
    cc>>=32;
  }
  if ((cc != 0 && Nat.incAt(7,z,4) != 0) || (z[6] == P6 && Nat224.gte(z,P))) {
    addPInvTo(z);
  }
}
