{
  long c=0, xVal=x & M;
  long y00=y & M;
  c+=xVal * y00 + (z[zOff + 0] & M);
  z[zOff + 0]=(int)c;
  c>>>=32;
  long y01=y >>> 32;
  c+=xVal * y01 + y00 + (z[zOff + 1] & M);
  z[zOff + 1]=(int)c;
  c>>>=32;
  c+=y01 + (z[zOff + 2] & M);
  z[zOff + 2]=(int)c;
  c>>>=32;
  c+=(z[zOff + 3] & M);
  z[zOff + 3]=(int)c;
  c>>>=32;
  return c == 0 ? 0 : Nat.incAt(8,z,zOff,4);
}
