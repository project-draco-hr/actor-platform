{
  this.dataAdapter=dataAdapter;
  this.comparator=new Comparator<V>(){
    @Override public int compare(    V lhs,    V rhs){
      long lKey=dataAdapter.getSortKey(lhs);
      long rKey=dataAdapter.getSortKey(rhs);
      if (lKey > rKey) {
        return -1;
      }
 else       if (lKey < rKey) {
        return 1;
      }
 else {
        return 0;
      }
    }
  }
;
}
