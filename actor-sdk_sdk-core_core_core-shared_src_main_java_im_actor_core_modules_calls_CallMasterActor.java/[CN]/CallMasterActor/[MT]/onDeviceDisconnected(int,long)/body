{
  MasterCallMember member=members.filter(MasterCallMember.PREDICATE(uid,deviceId)).firstOrNull();
  if (member == null) {
    return;
  }
  member.getDeviceId().remove(deviceId);
  if (member.getDeviceId().size() == 0) {
    members.remove(member);
  }
  if (members.isEmpty()) {
    shutdown();
    return;
  }
  updateMembers();
}
