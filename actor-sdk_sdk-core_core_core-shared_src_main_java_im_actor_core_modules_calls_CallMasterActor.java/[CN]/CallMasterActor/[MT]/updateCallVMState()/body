{
  debugState();
  if (callVM.getState().get() == CallState.ENDED) {
    return;
  }
  if (state.getMembers().isAll(MasterCallMember.IS_ENDED)) {
    shutdown();
    return;
  }
  if (state.getMembers().isAny(MasterCallMember.IS_IN_PROGRESS)) {
    callVM.getState().change(CallState.IN_PROGRESS);
    return;
  }
  if (state.getMembers().isAny(MasterCallMember.IS_CONNECTING)) {
    callVM.getState().change(CallState.CONNECTING);
    return;
  }
}
