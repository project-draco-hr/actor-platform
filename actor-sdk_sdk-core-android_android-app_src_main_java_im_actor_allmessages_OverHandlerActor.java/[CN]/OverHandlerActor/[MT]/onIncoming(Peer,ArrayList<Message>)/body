{
  ArrayList<MessageEx> msgex=new ArrayList<MessageEx>();
  for (  Message m : msgs) {
    boolean containsMyLike=false;
    for (    Reaction r : m.getReactions()) {
      if (r.getUids().contains(myUid())) {
        containsMyLike=true;
        break;
      }
    }
    if (containsMyLike) {
      msgex.add(new MessageEx(m,peer));
    }
  }
  allMessages.addOrUpdateItems(msgex);
}
