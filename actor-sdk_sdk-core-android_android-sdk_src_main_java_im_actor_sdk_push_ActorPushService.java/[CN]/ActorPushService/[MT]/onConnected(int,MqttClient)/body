{
  if (this.attemptIndex == attempt) {
    this.isConnecting=false;
    this.mqttClient=mqttClient;
  }
 else {
    try {
      mqttClient.disconnect();
    }
 catch (    MqttException e) {
      e.printStackTrace();
    }
    try {
      mqttClient.close();
    }
 catch (    MqttException e) {
      e.printStackTrace();
    }
  }
}
