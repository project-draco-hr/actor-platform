{
  context().getMessagesModule().getDialogsActor().send(new DialogsActor.UserChanged(u));
  context().getMessagesModule().getDialogsGroupedActor().send(new GroupedDialogsActor.PeerInformationChanged(Peer.user(u.getUid()),u.getName(),u.getAvatar()));
  context().getContactsModule().getContactSyncActor().send(new ContactsSyncActor.UserChanged(u));
}
