{
  StringBuffer buf=new StringBuffer();
  String sep=System.getProperty("line.separator");
  buf.append("CRLDistPoint:");
  buf.append(sep);
  DistributionPoint dp[]=getDistributionPoints();
  for (int i=0; i != dp.length; i++) {
    buf.append("    ");
    buf.append(dp[i]);
    buf.append(sep);
  }
  return buf.toString();
}
