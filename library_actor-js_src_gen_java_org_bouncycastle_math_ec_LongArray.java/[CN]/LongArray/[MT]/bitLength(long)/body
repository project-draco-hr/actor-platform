{
  int u=(int)(w >>> 32), b;
  if (u == 0) {
    u=(int)w;
    b=0;
  }
 else {
    b=32;
  }
  int t=u >>> 16, k;
  if (t == 0) {
    t=u >>> 8;
    k=(t == 0) ? bitLengths[u] : 8 + bitLengths[t];
  }
 else {
    int v=t >>> 8;
    k=(v == 0) ? 16 + bitLengths[t] : 24 + bitLengths[v];
  }
  return b + k;
}
