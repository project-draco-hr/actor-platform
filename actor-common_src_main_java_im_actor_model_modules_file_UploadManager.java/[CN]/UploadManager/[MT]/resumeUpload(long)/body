{
  QueueItem queueItem=findItem(rid);
  if (queueItem != null) {
    if (queueItem.isStarted) {
      return;
    }
    if (queueItem.isStopped) {
      queueItem.isStopped=false;
    }
    queueItem.progress=0;
    for (    UploadCallback callback : queueItem.callbacks) {
      callback.onUploading(0);
    }
    checkQueue();
  }
}
