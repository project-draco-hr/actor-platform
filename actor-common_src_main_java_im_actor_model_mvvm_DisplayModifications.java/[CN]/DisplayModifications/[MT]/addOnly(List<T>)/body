{
  return new DisplayList.Modification<T>(){
    @Override public void modify(    List<T> sourceList){
      ArrayList<T> toAdd=new ArrayList<T>();
      outer:       for (      T t : items) {
        for (        T srcT : sourceList) {
          if (srcT.getEngineId() == t.getEngineId()) {
            continue outer;
          }
        }
        toAdd.add(t);
      }
      sourceList.addAll(toAdd);
    }
  }
;
}
