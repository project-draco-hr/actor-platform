{
  DialogSpec spec=new DialogSpec(peer,false,counter);
  specs.getEngine().addOrUpdateItem(spec);
  boolean found=false;
  for (  Peer p : storage.getPrivatePeers()) {
    if (p.equals(peer)) {
      found=true;
      break;
    }
  }
  for (  Peer p : storage.getGroupPeers()) {
    if (p.equals(peer)) {
      found=true;
      break;
    }
  }
  if (found) {
    notifyVM();
  }
}
