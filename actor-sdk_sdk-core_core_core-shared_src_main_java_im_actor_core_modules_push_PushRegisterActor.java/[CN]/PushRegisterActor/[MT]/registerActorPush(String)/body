{
  if (preferences().getBool("push.actor",false) && preferences().getBool("push.actor.registered",false) && endpoint.equals(preferences().getString("push.actor.endpoint"))) {
    return;
  }
  preferences().putBool("push.actor",true);
  preferences().putBool("push.actor.registered",false);
  preferences().putString("push.actor.endpoint",endpoint);
  request(new RequestRegisterActorPush(endpoint,new ArrayList<>()),new RpcCallback<ResponseVoid>(){
    @Override public void onResult(    ResponseVoid response){
      preferences().putBool("push.actor.registered",true);
    }
    @Override public void onError(    RpcException e){
    }
  }
);
}
