{
  if (reverse) {
    for (int i=possRDNs.length - 1; i >= 0; i--) {
      if (possRDNs[i] != null && rdnAreEqual(rdn,possRDNs[i])) {
        possRDNs[i]=null;
        return true;
      }
    }
  }
 else {
    for (int i=0; i != possRDNs.length; i++) {
      if (possRDNs[i] != null && rdnAreEqual(rdn,possRDNs[i])) {
        possRDNs[i]=null;
        return true;
      }
    }
  }
  return false;
}
