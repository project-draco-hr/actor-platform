{
  m[pt]=1;
  for (int i=0; i < pt; i++) {
    m[i]=0;
  }
  streebog_g(h,m,n);
  int c=0;
  for (int i=63; i >= 0; i--) {
    c+=(e[i] & 0xFF) + (m[i] & 0xFF);
    e[i]=(byte)(c & 0xFF);
    c>>=8;
  }
  for (int j=0; j < 64; j++) {
    m[j]=(byte)0x00;
  }
  n+=(63 - pt) << 3;
  for (int i=63; n > 0; i--) {
    m[i]=(byte)(n & 0xFF);
    n>>=8;
  }
  streebog_g(h,m,0);
  streebog_g(h,e,0);
  for (int j=0; j < hashLength; j++) {
    out[offset + j]=h[j];
  }
  reset();
}
