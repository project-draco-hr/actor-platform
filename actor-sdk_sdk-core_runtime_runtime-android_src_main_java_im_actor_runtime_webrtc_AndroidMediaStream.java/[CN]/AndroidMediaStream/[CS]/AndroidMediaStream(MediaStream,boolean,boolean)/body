{
  this.local=local;
  this.stream=stream;
  if (!local) {
    audioTrack=stream.audioTracks.get(0);
  }
 else {
    AudioSource audioSource=factory().createAudioSource(new MediaConstraints());
    audioTrack=factory().createAudioTrack("ARDAMSa0",audioSource);
    stream.addTrack(audioTrack);
  }
  setEnabled(autoPlay);
}
