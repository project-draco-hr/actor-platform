{
  if (p == null) {
    throw new IllegalArgumentException("'p' cannot be null");
  }
  if (g == null) {
    throw new IllegalArgumentException("'g' cannot be null");
  }
  if (q == null) {
    throw new IllegalArgumentException("'q' cannot be null");
  }
  this.p=p;
  this.g=g;
  this.q=q;
  this.j=j;
  this.validationParms=validationParms;
}
