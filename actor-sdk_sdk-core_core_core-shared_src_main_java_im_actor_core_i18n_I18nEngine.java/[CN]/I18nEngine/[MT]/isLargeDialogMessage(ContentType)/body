{
switch (contentType) {
case SERVICE:
case SERVICE_AVATAR:
case SERVICE_AVATAR_REMOVED:
case SERVICE_CREATED:
case SERVICE_TITLE:
case SERVICE_LEAVE:
case SERVICE_REGISTERED:
case SERVICE_KICK:
case SERVICE_ADD:
case SERVICE_JOINED:
case SERVICE_CALL_ENDED:
case SERVICE_CALL_MISSED:
case SERVICE_ABOUT:
case SERVICE_TOPIC:
    return true;
default :
  return false;
}
}
