{
  if (item.getItemId() == R.id.done) {
    execute(messenger().createGroup(title,BoxUtil.unbox(selectedUsers.toArray(new Integer[0]))),R.string.progress_common,new CommandCallback<Integer>(){
      @Override public void onResult(      Integer res){
        getActivity().startActivity(Intents.openGroupDialog(res,true,getActivity()));
        getActivity().finish();
      }
      @Override public void onError(      Exception e){
      }
    }
);
    return true;
  }
  return super.onOptionsItemSelected(item);
}
