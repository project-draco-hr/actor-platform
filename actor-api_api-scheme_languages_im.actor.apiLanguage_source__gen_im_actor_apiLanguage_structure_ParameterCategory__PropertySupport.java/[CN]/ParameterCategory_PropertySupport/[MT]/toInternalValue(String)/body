{
  if (value == null) {
    return null;
  }
  Iterator<ParameterCategory> constants=ListSequence.fromList(ParameterCategory.getConstants()).iterator();
  while (constants.hasNext()) {
    ParameterCategory constant=constants.next();
    if (value.equals(constant.getName())) {
      return constant.getValueAsString();
    }
  }
  return null;
}
