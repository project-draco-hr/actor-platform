{
  this.persistenceMap=persistenceMap;
  this.backedSet=new HashSet<V>();
  this.mapping=new HashMap<V,Long>();
  for (  Long key : persistenceMap.keySet()) {
    V val=persistenceMap.get(key);
    backedSet.add(val);
    mapping.put(val,key);
    if (key > nextId) {
      nextId=key + 1;
    }
  }
}
