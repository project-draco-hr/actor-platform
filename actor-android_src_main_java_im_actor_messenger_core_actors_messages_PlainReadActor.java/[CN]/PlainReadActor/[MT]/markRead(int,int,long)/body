{
  long uid=DialogUids.getDialogUid(chatType,chatId);
  if (read.containsKey(uid)) {
    if (read.get(uid) > date) {
      return;
    }
  }
  if (pending.containsKey(uid)) {
    if (pending.get(uid) > date) {
      return;
    }
  }
  pending.put(uid,date);
  peformMarkread(chatType,chatId,date);
}
