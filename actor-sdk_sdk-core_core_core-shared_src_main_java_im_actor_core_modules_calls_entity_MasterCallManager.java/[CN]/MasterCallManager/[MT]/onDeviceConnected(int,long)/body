{
  MasterCallMember member=connectedMembers.filter(MasterCallMember.PREDICATE(uid)).firstOrNull();
  if (member == null) {
    return false;
  }
  if (!connectedDevices.filter(MasterCallDevice.PREDICATE(uid,deviceId)).isEmpty()) {
    return false;
  }
  connectedDevices.add(new MasterCallDevice(uid,deviceId,false));
  if (member.getState() == MasterCallMemberState.RINGING) {
    member.setState(MasterCallMemberState.RINGING_REACHED);
  }
  return true;
}
