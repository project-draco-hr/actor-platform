{
  MasterCallDevice device=connectedDevices.filter(MasterCallDevice.PREDICATE(uid,deviceId)).firstOrNull();
  if (device == null) {
    return false;
  }
  MasterCallMember member=connectedMembers.filter(MasterCallMember.PREDICATE(uid)).firstOrNull();
  if (member == null) {
    return false;
  }
switch (member.getState()) {
case CONNECTING:
    member.setState(MasterCallMemberState.IN_PROGRESS);
  break;
}
if (isAnswered) {
return false;
}
isAnswered=true;
return true;
}
