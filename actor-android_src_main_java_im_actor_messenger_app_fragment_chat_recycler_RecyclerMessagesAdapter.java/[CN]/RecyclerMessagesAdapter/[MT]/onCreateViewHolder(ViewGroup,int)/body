{
switch (viewType) {
case 0:
    return new FooterHolder(messagesFragment.getActivity());
case 1:
  return new TextHolder(messagesFragment,inflate(R.layout.adapter_dialog_text,viewGroup));
case 2:
return new ServiceHolder(messagesFragment,inflate(R.layout.adapter_dialog_service,viewGroup));
case 3:
return new PhotoHolder(messagesFragment,inflate(R.layout.adapter_dialog_photo,viewGroup));
case 4:
return new DocHolder(messagesFragment,inflate(R.layout.adapter_dialog_doc,viewGroup));
}
throw new RuntimeException("Unknown view type");
}
