{
  if (peer.getType() == PeerType.PRIVATE) {
    startTyping(uid);
    self().sendOnce(new CancelTyping(DialogType.TYPE_USER,peer.getId(),uid),TYPING_TEXT_TIMEOUT);
  }
 else   if (peer.getType() == PeerType.GROUP) {
    startTyping(peer.getId(),uid);
    self().sendOnce(new CancelTyping(DialogType.TYPE_GROUP,peer.getId(),uid),TYPING_TEXT_TIMEOUT);
  }
}
