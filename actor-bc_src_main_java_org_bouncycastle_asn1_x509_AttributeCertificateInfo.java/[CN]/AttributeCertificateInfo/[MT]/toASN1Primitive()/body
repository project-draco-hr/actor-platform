{
  ASN1EncodableVector v=new ASN1EncodableVector();
  if (version.getValue().intValue() != 0) {
    v.add(version);
  }
  v.add(holder);
  v.add(issuer);
  v.add(signature);
  v.add(serialNumber);
  v.add(attrCertValidityPeriod);
  v.add(attributes);
  if (issuerUniqueID != null) {
    v.add(issuerUniqueID);
  }
  if (extensions != null) {
    v.add(extensions);
  }
  return new DERSequence(v);
}
