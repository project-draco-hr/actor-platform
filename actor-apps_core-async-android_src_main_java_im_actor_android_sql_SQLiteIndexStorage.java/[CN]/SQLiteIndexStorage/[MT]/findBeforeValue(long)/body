{
  checkSqlite();
  List<Long> list=new ArrayList<Long>();
  Cursor cursor=db.query("\"" + name + "\"",new String[]{"\"VALUE\""},"\"VALUE\" < ?",new String[]{"" + value},null,null,null);
  if (cursor == null) {
    return list;
  }
  try {
    if (cursor.moveToFirst()) {
      do {
        list.add(cursor.getLong(0));
      }
 while (cursor.moveToNext());
      return list;
    }
  }
  finally {
    cursor.close();
  }
  return list;
}
