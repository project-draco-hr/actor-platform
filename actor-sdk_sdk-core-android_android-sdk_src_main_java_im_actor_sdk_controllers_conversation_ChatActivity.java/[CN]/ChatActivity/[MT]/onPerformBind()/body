{
  super.onPerformBind();
  if (peer.getPeerType() == PeerType.PRIVATE) {
    final UserVM user=users().get(peer.getPeerId());
    if (user == null) {
      finish();
      return;
    }
  }
 else   if (peer.getPeerType() == PeerType.GROUP) {
    GroupVM group=groups().get(peer.getPeerId());
    if (group == null) {
      finish();
      return;
    }
  }
}
