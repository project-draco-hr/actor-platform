{
  long c=(z[0] & M) - 1;
  z[0]=(int)c;
  c>>=32;
  if (c != 0) {
    c+=(z[1] & M);
    z[1]=(int)c;
    c>>=32;
    c+=(z[2] & M);
    z[2]=(int)c;
    c>>=32;
  }
  c+=(z[3] & M) + 1;
  z[3]=(int)c;
  c>>=32;
  if (c != 0) {
    c+=(z[4] & M);
    z[4]=(int)c;
    c>>=32;
    c+=(z[5] & M);
    z[5]=(int)c;
    c>>=32;
  }
  c+=(z[6] & M) + 1;
  z[6]=(int)c;
  c>>=32;
  c+=(z[7] & M) - 1;
  z[7]=(int)c;
}
