{
  this.map=new HashMap<String,Object>();
  if (map != null) {
    Iterator<Entry<String,Object>> i=map.entrySet().iterator();
    while (i.hasNext()) {
      Entry<String,Object> entry=i.next();
      Object value=entry.getValue();
      if (value != null) {
        this.map.put(entry.getKey(),value);
      }
    }
  }
}
