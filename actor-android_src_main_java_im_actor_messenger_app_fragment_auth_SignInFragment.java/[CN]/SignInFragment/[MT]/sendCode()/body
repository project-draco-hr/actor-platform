{
  String text=smsCodeEnterEditText.getText().toString().trim();
  if (text.length() > 0) {
    int code;
    try {
      code=Integer.parseInt(smsCodeEnterEditText.getText().toString());
    }
 catch (    Exception e) {
      e.printStackTrace();
      return;
    }
    execute(auth().sendCode(code));
  }
}
