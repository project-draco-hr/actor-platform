{
  long c=(z[0] & M) - 1;
  z[0]=(int)c;
  c>>=32;
  c+=(z[1] & M) + 1;
  z[1]=(int)c;
  c>>=32;
  if (c != 0) {
    c+=(z[2] & M);
    z[2]=(int)c;
    c>>=32;
  }
  c+=(z[3] & M) - 1;
  z[3]=(int)c;
  c>>=32;
  c+=(z[4] & M) - 1;
  z[4]=(int)c;
  c>>=32;
  if (c != 0) {
    Nat.decAt(12,z,5);
  }
}
