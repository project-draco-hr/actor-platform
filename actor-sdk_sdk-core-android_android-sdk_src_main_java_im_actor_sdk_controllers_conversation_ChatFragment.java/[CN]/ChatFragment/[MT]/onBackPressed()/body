{
  AbsAttachFragment attachFragment=findShareFragment();
  if (attachFragment != null) {
    if (attachFragment.onBackPressed()) {
      return true;
    }
  }
  if (editRid != 0) {
    editRid=0;
    findInputBar().setText("");
    hideQuote();
    return true;
  }
  if (currentQuote != null) {
    currentQuote=null;
    hideQuote();
    return true;
  }
  return false;
}
