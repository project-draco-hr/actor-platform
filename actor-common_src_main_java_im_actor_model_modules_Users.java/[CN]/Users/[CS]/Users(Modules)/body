{
  super(messenger);
  this.collection=new MVVMCollection<User,UserVM>(messenger.getConfiguration().getStorage().createKeyValue(STORAGE_USERS)){
    @Override protected UserVM createNew(    User raw){
      return new UserVM(raw,modules());
    }
    @Override protected byte[] serialize(    User raw){
      return raw.toByteArray();
    }
    @Override protected User deserialize(    byte[] raw){
      try {
        return User.fromBytes(raw);
      }
 catch (      IOException e) {
        e.printStackTrace();
        return null;
      }
    }
  }
;
  this.users=collection.getEngine();
}
