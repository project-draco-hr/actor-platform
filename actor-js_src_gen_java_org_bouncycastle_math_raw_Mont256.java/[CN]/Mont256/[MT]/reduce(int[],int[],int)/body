{
  for (int i=0; i < 8; ++i) {
    int z_0=z[0];
    long t=(z_0 * mInv32) & M;
    long carry=t * (m[0] & M) + (z_0 & M);
    carry>>>=32;
    for (int j=1; j < 8; ++j) {
      carry+=t * (m[j] & M) + (z[j] & M);
      z[j - 1]=(int)carry;
      carry>>>=32;
    }
    z[7]=(int)carry;
  }
  if (Nat256.gte(z,m)) {
    Nat256.sub(z,m,z);
  }
}
