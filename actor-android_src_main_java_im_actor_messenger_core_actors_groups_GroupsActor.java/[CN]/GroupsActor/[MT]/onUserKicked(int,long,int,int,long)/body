{
  GroupModel groupInfo=groups().get(chatId);
  if (groupInfo == null || groupInfo.getState() != GroupState.JOINED) {
    return;
  }
  if (uid == myUid()) {
    groups().put(chatId,groupInfo.getRaw().updateState(GroupState.KICKED).updateMembers(new ArrayList<GroupMember>()));
  }
 else {
    groups().put(chatId,groupInfo.getRaw().updateMembers(removeItem(groupInfo.getRaw().getMembers(),uid)));
  }
  ConversationActor.conv(DialogType.TYPE_GROUP,chatId).onInMessage(rid,kicker,date,new GroupKick(uid));
}
