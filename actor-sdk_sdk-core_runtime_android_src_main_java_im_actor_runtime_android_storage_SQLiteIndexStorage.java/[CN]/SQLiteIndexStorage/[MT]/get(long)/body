{
  checkSqlite();
  Cursor cursor=db.query("\"" + name + "\"",new String[]{"\"VALUE\""},"\"ID\" = ?",new String[]{"" + key},null,null,null);
  if (cursor == null) {
    return null;
  }
  try {
    if (cursor.moveToFirst()) {
      return cursor.getLong(0);
    }
  }
  finally {
    cursor.close();
  }
  return null;
}
