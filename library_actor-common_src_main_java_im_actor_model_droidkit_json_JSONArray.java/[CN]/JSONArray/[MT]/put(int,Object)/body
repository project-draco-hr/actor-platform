{
  im.actor.model.droidkit.json.JSONObject.testValidity(value);
  if (index < 0) {
    throw new JSONException("JSONArray[" + index + "] not found.");
  }
  if (index < this.length()) {
    this.myArrayList.set(index,value);
  }
 else {
    while (index != this.length()) {
      this.put(im.actor.model.droidkit.json.JSONObject.NULL);
    }
    this.put(value);
  }
  return this;
}
